{
    "name": "mobileia/auth",
    "description": "Simple library for MobileIA Users",
    "version": "0.0.9",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Matias Camiletti",
            "homepage": "http://www.mobileia.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/MobileIA/authentication"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zend-http": "^2.5",
        "zendframework/zend-json": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "MobileIA\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MobileIA\\Auth\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "master": "1.0"
        }
    }
}
